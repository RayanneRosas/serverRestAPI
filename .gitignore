 package-lock.json
steps:
- name: Checkout
  uses: actions/checkout@v4

- name: Install dependencies
  run: npm install

- name: Cypress install
  uses: cypress-io/github-action@v6
  with:
    install: true

- name: Run Cypress tests
  uses: cypress-io/github-action@v6
  with:
    runTests: true
